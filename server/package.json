{
  "name": "pf360-server",
  "version": "1.0.0",
  "description": "Backend API for PF360 application",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=development node -r dotenv/config app.js dotenv_config_path=.env.development",
    "dev:watch": "NODE_ENV=development nodemon -r dotenv/config app.js dotenv_config_path=.env.development",
    "start": "NODE_ENV=production node -r dotenv/config app.js dotenv_config_path=.env.production",
    "build": "echo 'No build step needed for Node.js'",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "carbone": "^3.5.6",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "express",
    "prisma",
    "jwt",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
